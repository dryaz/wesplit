{
  "hosting": {
    "public": "composeApp/build/dist/js/productionExecutable",
    "headers": [
      {
        "source": "/.well-known/apple-app-site-association",
        "headers": [{"key": "Content-Type", "value": "application/json"}]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5005,
      "host": "127.0.0.1"
    },
    "firestore": {
      "port": 8088,
      "host": "127.0.0.1"
    },
    "auth": {
      "host": "127.0.0.1",
      "port": 9099
    }
  }
}
